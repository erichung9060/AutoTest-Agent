{
  "mcpServers": {
    "mobile-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mobilenext/mobile-mcp@latest"
      ]
    },
    "sleep": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y", 
        "sleep-mcp"
      ]
    }
  }
}